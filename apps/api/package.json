{
  "name": "@trading-log/api",
  "version": "1.0.0",
  "description": "Trading Log API Server",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:switch:sqlite": "node scripts/switch-db.js sqlite",
    "db:switch:supabase": "node scripts/switch-db.js supabase",
    "db:setup:sqlite": "npm run db:switch:sqlite && npm run db:generate && npm run db:push && npm run db:seed",
    "db:setup:supabase": "npm run db:switch:supabase && npm run db:generate && npm run db:push && npm run db:seed",
    "db:seed": "tsx src/seed.ts",
    "db:test": "node scripts/test-connection.js",
    "db:status": "node scripts/db-status.js"
  },
  "dependencies": {
    "@trading-log/shared": "*",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "decimal.js": "^10.4.3",
    "zod": "^3.22.0",
    "prisma": "^5.0.0",
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.38.0",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}